{
    "name": "zikula/categories-module",
    "version": "3.1.0-dev",
    "description": "Categories administration",
    "type": "zikula-system-module",
    "license": "LGPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\CategoriesModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^5.2",
        "symfony/contracts": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/doctrine-bridge": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/form": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/security": "^5.2",
        "symfony/validator": "^5.2",

        "zikula/core-bundle": "self.version",
        "zikula/coreinstaller-bundle": "self.version",
        "zikula/formextension-bundle": "self.version",
        "zikula/extensions-module": "self.version",
        "zikula/menu-module": "self.version",
        "zikula/settings-module": "self.version",
        "zikula/permissions-module": "self.version",
        "zikula/theme-module": "self.version",
        "zikula/users-module": "self.version"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\CategoriesModule\\ZikulaCategoriesModule",
            "core-compatibility": ">=3.0.0",
            "displayname": "Categories",
            "url": "categories",
            "icon": "fas fa-sitemap",
            "capabilities": {
                "admin": {
                    "route": "zikulacategoriesmodule_category_listcategories"
                }
            },
            "securityschema": {
                "ZikulaCategoriesModule::": "::",
                "ZikulaCategoriesModule:PropertyId:CategoryId": "$propId::$catId"
            }
        }
    }
}
